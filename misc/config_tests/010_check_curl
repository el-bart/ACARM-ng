if grep -q '^filteripblacklist$' <<< "$PLUGINS"
then
  echo -n "checking for curl... "
  echo -e "#include <curl/curl.h>\nint main(void){ curl_global_init(0); return 0;}" > "$TMP"
  if compile "$TMP" `$SRCS/curlpp/features/deps/common_dynamic_link`
  then
    echo "ok"
  else
    echo "N/A"
    echo "install missing library or withdraw usage of plugins: filteripblacklist"
    rm -f "$TMP"
    exit 2
  fi
fi
