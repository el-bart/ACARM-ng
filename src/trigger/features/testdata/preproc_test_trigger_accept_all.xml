<?xml version="1.0" encoding="UTF-8"?>
<acarm_ng>

  <general>
    <url>http://test.url</url>
    <cleanupInterval>42</cleanupInterval>
    <cleanupOlder>13</cleanupOlder>
    <pluginsDir>/some/dir</pluginsDir>
    <alertsLimit>1983</alertsLimit>
  </general>

  <persistency>
    <type>stubs</type>
  </persistency>

  <logger>
    <appenders>
      <file name="default">
        <output>loggerfile.log</output>
      </file>
    </appenders>

    <nodes appender="default" threshold="debug">
    </nodes>
  </logger>

  <preprocessor/>

  <filters/>

  <triggers>
    <preproc_test_trigger name="triggername">
      <preprocessor>
        <accept>
          <true/>
        </accept>
      </preprocessor>
    </preproc_test_trigger>
  </triggers>

  <inputs/>

</acarm_ng>
