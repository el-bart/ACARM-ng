<?xml version="1.0" encoding="UTF-8"?>
<acarm_ng>

  <general>
    <url>http://test.url</url>
    <cleanupInterval>42</cleanupInterval>
    <cleanupOlder>13</cleanupOlder>
    <pluginsDir>/some/dir</pluginsDir>
    <memoryLimit>1983</memoryLimit>
  </general>

  <persistency>
    <type>persistencytest</type>
  </persistency>

  <logger>
    <appenders>
      <file name="default">
        <output>acarm-ng.log</output>
      </file>
    </appenders>

    <nodes appender="default" threshold="debug">
    </nodes>
  </logger>

  <preprocessor/>

  <filters/>

  <triggers/>

  <inputs/>

</acarm_ng>
