<?xml version="1.0" encoding="UTF-8"?>
<acarm_ng>

  <general>
    <url>http://test.url</url>
    <cleanupInterval>42</cleanupInterval>
    <cleanupOlder>13</cleanupOlder>
    <pluginsDir>/some/dir</pluginsDir>
    <alertsLimit>1983</alertsLimit>
  </general>

  <persistency>
    <type>stubs</type>
  </persistency>

  <logger>
    <appenders>
      <file name="default">
        <output>loggerfile.log</output>
      </file>
    </appenders>

    <nodes appender="default" threshold="debug">
    </nodes>
  </logger>

  <preprocessor/>

  <filters/>

  <triggers>
    <jabber name="inform user">
      <login>acarm-ng</login>
      <server>jabber.com</server>
      <password>secret</password>
<!--
      <receiver_uin>4321</receiver_uin>
-->
    </jabber>
  </triggers>

  <inputs/>

</acarm_ng>
