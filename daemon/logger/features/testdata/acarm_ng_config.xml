<?xml version="1.0" encoding="UTF-8"?>
<acarm_ng>

  <persistency>
    <type>stubs</type>
  </persistency>

  <logger>
    <appenders>
      <file name="default">
        <output>loggerfile.log</output>
      </file>
      <null name="void" />
    </appenders>

    <nodes appender="default" threshold="debug">
      <subtree1>
        <subappender appender="void" />
        <subthreshold threshold="warn" />
        <both appender="void" threshold="fatal">
          <derivedend/>
          <updatethr threshold="info"/>
        </both>
      </subtree1>
    </nodes>
  </logger>

  <preprocessor/>

  <filters/>

  <triggers/>

  <inputs/>

</acarm_ng>
