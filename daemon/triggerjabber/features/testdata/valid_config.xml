<?xml version="1.0" encoding="UTF-8"?>
<acarm_ng>

  <general>
    <url>http://test.url</url>
    <cleanupInterval>42</cleanupInterval>
  </general>

  <persistency>
    <type>stubs</type>
  </persistency>

  <logger>
    <appenders>
      <file name="default">
        <output>loggerfile.log</output>
      </file>
    </appenders>

    <nodes appender="default" threshold="debug">
    </nodes>
  </logger>

  <preprocessor/>

  <filters/>

  <triggers>
    <jabber name="inform user">
      <login>acarm-ng</login>
      <server>jabber.com</server>
      <password>secret</password>
      <receivers>reciever</receivers>
      <severity_threshold>3.14</severity_threshold>
      <alerts_count_threshold>42</alerts_count_threshold>
    </jabber>
  </triggers>

  <inputs/>

</acarm_ng>
