<?xml version="1.0" encoding="UTF-8"?>
<acarm_ng>

  <persistency>
    <type>stubs</type>
  </persistency>

  <logger>
    <appenders>
      <file name="default">
        <output>loggerfile.log</output>
      </file>
    </appenders>

    <nodes appender="default" threshold="debug">
    </nodes>
  </logger>

  <preprocessor/>

  <filters/>

  <triggers>
    <file>
      <outdir>.</outdir>
      <severity_threshold>3.14</severity_threshold>
      <alerts_count_threshold>42</alerts_count_threshold>
    </file>
  </triggers>

  <inputs/>

</acarm_ng>
